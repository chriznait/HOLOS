



UPDATE otm_registro SET 
fecha='2020-12-01',
fechaSolicita = concat('2020-12-01', time(fechaSolicita)),
fechsmpmnicio = concat('2020-12-01', time(fechsmpmnicio)),
fechaFin = concat('2020-12-01', time(fechaFin)),
fechaJefeArea = concat('2020-12-01', time(fechaJefeArea)),
fechaJefeMantenimiento = concat('2020-12-01', time(fechaJefeMantenimiento)),
fechaHora = concat('2020-12-01', time(fechaHora)),
actualizacion = concat('2020-12-01', time(actualizacion)),

where idRegistro=2660 


